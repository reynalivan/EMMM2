{
  "identifier": "default",
  "description": "Default capability for the application",
  "windows": ["main"],
  "webviews": ["main", "browser-tab-*"],
  "permissions": [
    "core:default",
    "core:webview:default",
    "opener:default",
    "log:default",
    "dialog:default",
    "fs:default",
    "core:path:default",
    "core:window:allow-set-title",
    "fs:read-all",
    "fs:write-all",
    "allow-browser-commands",
    "allow-app-commands"
  ]
}
